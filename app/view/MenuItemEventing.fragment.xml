<core:FragmentDefinition
    xmlns="sap.m"
    xmlns:core="sap.ui.core"
    xmlns:u="sap.ui.unified"
    controllerName="myapp.controller.MenuItemEventing">
    <u:Menu items="{path: '/menu'}" itemSelect="visuBatch">
        <u:MenuItem
            text="{nome}"
            startsSection="{nuovasezione}"
            enabled="{attivo}"
        />
       
    </u:Menu>

</core:FragmentDefinition>