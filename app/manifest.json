{
    "_version": "1.1.0",
    "sap.app": {
        "id": "myapp",
        "type": "application",
        "i18n": "locales/locale.properties",
        "title": "{{myapp.title}}",
        "description": "{{myapp.description}}",
        "applicationVersion": {
            "version": "0.0.1"
        },
        "offline": true
    },
    "sap.ui": {
        "_version": "1.1.0",
        "technology": "UI5",
        "icons": {
            "icon": "",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": false
        },
        "supportedThemes": [
            "sap_hcb",
            "sap_bluecrystal"
        ]
    },
    "sap.ui5": {
        "_version": "1.1.0",
        "resources": {
            "css": [{
                    "uri": "css/style.css",
                    "id": "customstyle"
                },
                {
                    "uri": "css/styleDeCecco.css",
                    "id": "styleDeCecco"
                }]
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        },
        "models": {
            "locale": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "myapp.locales.locale"
                }
            }
        },
        "dependencies": {
            "minUI5Version": "1.30.0",
            "libs": {
                "sap.m": {
                    "minVersion": "1.30.0"
                },
                "sap.ui.commons": {
                    "minVersion": "1.30.0"
                },
                "sap.ui.layout": {
                    "minVersion": "1.30.0"
                }
            }
        },
        "rootView": "myapp.view.App",
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "viewPath": "myapp",
                "controlId": "rootControl",
                "controlAggregation": "pages",
                "viewType": "XML"
            },
            "routes": [{
                    "name": "main",
                    "pattern": "",
                    "target": "main"
                }, {
                    "name": "piani",
                    "pattern": "piani",
                    "target": "piani"
                }, {
                    "name": "managePiano",
                    "pattern": "managePiano/{turnoPath}/{pianoPath}",
                    "target": "managePiano"
                }, {
                    "name": "managePianoGrey",
                    "pattern": "managePianoGrey/{turnoPath}/{pianoPath}",
                    "target": "managePianoGrey"
                }, {
                    "name": "Report",
                    "pattern": "managePianoGrey/{turnoPath}/{pianoPath}/Report",
                    "target": "Report"                    
                }, {
                    "name": "guastidilinea",
                    "pattern": "managePianoGrey/{turnoPath}/{pianoPath}/Report/{guastiPath}",
                    "target": "guastiLinea"
                }, {
                    "name": "RiepilogoLinee",
                    "pattern": "RiepilogoLinee",
                    "target": "RiepilogoLinee"
                }, {
                    "name": "sinotticoLinea",
                    "pattern": "sinotticoLinea/{lineaPath}",
                    "target": "sinotticoLinea"
                }],
            "targets": {
                "main": {
                    "viewName": "view.Main",
                    "viewLevel": 0
                },
                "piani": {
                    "viewName": "view.Piani"
                },
                "managePiano": {
                    "viewName": "view.managePiano"
                },
                "managePianoGrey": {
                    "viewName": "view.ManagePianoGrey"
                },
                "Report": {
                    "viewName": "view.Report"
                },
                "guastiLinea": {
                    "viewName": "view.guastiLinea"
                },
                "RiepilogoLinee": {
                    "viewName": "view.RiepilogoLinee"
                },
                "sinotticoLinea": {
                    "viewName": "view.SinotticiDiLinea"
                }
            }
        },
        "config": {
            "remoteDS": "/sap/opu/odata/SAP/ZAPP1_SRV/"
        }
    }
}